{
  "name": "link-card",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "run-p --aggregate-output --continue-on-error --print-label 'lint:*'",
    "lint:eslint": "eslint --color --ext .js,.ts,.jsx,.tsx .",
    "lint:prettier": "FORCE_COLOR=1 prettier --check .",
    "format": "run-s --print-label format:eslint format:prettier",
    "format:eslint": "eslint --color --ext .js,.ts,.jsx,.tsx --fix .",
    "format:prettier": "FORCE_COLOR=1 prettier --write .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "ky": "^0.33.2",
    "next": "^13.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@vitest/coverage-v8": "^0.34.1",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.6",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "4.9.5",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "packageManager": "yarn@3.6.0"
}
